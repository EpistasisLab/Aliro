{
  "name": "pennai-webapp",
  "version": "1.0.0",
  "description": "your friendly machine learning assistant",
  "scripts": {
    "build": "webpack",
    "build-dev": "webpack --watch --mode=development",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/EpistasisLab/pennai.git"
  },
  "keywords": [
    "machine learning"
  ],
  "author": "University of Pennsylvania",
  "license": "GPL 3.0",
  "bugs": {
    "url": "https://github.com/EpistasisLab/pennai/issues"
  },
  "homepage": "https://github.com/EpistasisLab/pennai",
  "dependencies": {
    "c3": "^0.4.11",
    "d3": "^5.9.2",
    "es6-promise": "^4.2.8",
    "isomorphic-fetch": "^2.2.1",
    "moment": "^2.22.2",
    "papaparse": "^4.6.0",
    "plotly.js": "^1.49.2",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-plotly.js": "^2.3.0",
    "react-redux": "^5.0.2",
    "react-responsive": "^1.2.7",
    "react-router": "^3.0.2",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.1",
    "semantic-ui-react": "^0.78.2",
    "socket.io-client": "^2.0.3",
    "twix": "^1.1.5"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.6",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-15": "^1.4.0",
    "fetch-mock": "^7.3.1",
    "jest": "^24.8.0",
    "jest-fetch-mock": "^2.1.1",
    "jest-html-reporter": "^2.4.1",
    "jest-junit": "^6.4.0",
    "jest-react-native": "^18.0.0",
    "react-native": "^0.60.0-rc.1",
    "react-test-renderer": "^15.4.2",
    "redux-logger": "^3.0.6",
    "redux-mock-store": "^1.5.3",
    "webpack": "^4.18.1",
    "webpack-cli": "^3.1.0"
  },
  "jest": {
    "transform": {
      "^.+\\.jsx?$": "babel-jest",
      "^.+\\.js?$": "babel-jest",
      "^.+\\.ts?$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(fetch-mock)/)"
    ],
    "reporters": [
      "default",
      [
        "./node_modules/jest-html-reporter",
        {
          "pageTitle": "Jest Unit Test Suite",
          "includeFailureMsg": true,
          "includeConsoleLog": true,
          "outputPath": "/target/test-reports/html/unit_jest_test_report.html"
        }
      ],
      [
        "./node_modules/jest-junit",
        {
          "suiteName": "Jest Unit Test Suite",
          "outputDirectory": "/target/test-reports",
          "outputName": "unit_jest_xunit.xml",
          "classNameTemplate": "JestWebUnit.{classname}",
          "titleTemplate": "{title}"
        }
      ]
    ]
  }
}
