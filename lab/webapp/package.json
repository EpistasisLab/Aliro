{
  "name": "pennai-webapp",
  "version": "1.0.0",
  "description": "your friendly machine learning assistant",
  "scripts": {
    "build": "webpack",
    "build-dev": "webpack --watch",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/EpistasisLab/pennai.git"
  },
  "keywords": [
    "machine learning"
  ],
  "author": "University of Pennsylvania",
  "license": "GPL 3.0",
  "bugs": {
    "url": "https://github.com/EpistasisLab/pennai/issues"
  },
  "homepage": "https://github.com/EpistasisLab/pennai",
  "dependencies": {
    "c3": "^0.4.24",
    "es6-promise": "^4.2.8",
    "isomorphic-fetch": "^2.2.1",
    "moment": "^2.24.0",
    "papaparse": "^4.6.3",
    "react": "^15.6.2",
    "react-dom": "^15.6.2",
    "react-redux": "^5.1.2",
    "react-responsive": "^1.3.4",
    "react-router": "^3.2.6",
    "redux": "^3.7.2",
    "redux-thunk": "^2.3.0",
    "reselect": "^3.0.1",
    "semantic-ui-react": "^0.78.3",
    "socket.io-client": "^2.3.0",
    "twix": "^1.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-react": "^7.9.4",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.1.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-15": "^1.4.1",
    "fetch-mock": "^7.7.3",
    "jest": "^24.9.0",
    "jest-fetch-mock": "^2.1.2",
    "jest-html-reporter": "^2.8.2",
    "jest-junit": "^6.4.0",
    "jest-react-native": "^18.0.0",
    "react-native": "^0.60.6",
    "react-test-renderer": "^15.6.2",
    "redux-logger": "^3.0.6",
    "redux-mock-store": "^1.5.4",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "clean-webpack-plugin": "^3.0.0",
    "html-webpack-plugin": "^3.2.0",
    "html-webpack-template": "^6.2.0",
    "typescript": "^3.8.3"
  },
  "jest": {
    "transform": {
      "^.+\\.jsx?$": "babel-jest",
      "^.+\\.js?$": "babel-jest",
      "^.+\\.ts?$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(fetch-mock)/)"
    ],
    "reporters": [
      "default",
      [
        "./node_modules/jest-html-reporter",
        {
          "pageTitle": "Jest Webapp Unit Test Suite",
          "includeFailureMsg": true,
          "includeConsoleLog": true,
          "outputPath": "../../target/test-reports/html/unit_webapp_jest_test_report.html"
        }
      ],
      [
        "./node_modules/jest-junit",
        {
          "suiteName": "Jest Unit Test Suite",
          "outputDirectory": "/target/test-reports",
          "outputName": "unit_jest_xunit.xml",
          "classNameTemplate": "JestWebUnit.{classname}",
          "titleTemplate": "{title}"
        }
      ]
    ]
  }
}
