    "default": 0.1
 },
 "mutation_rate": {
    "description": " mutation rate, 0.0<=f<=1.0, [default = 0.05]",
    "type": "float",
    "default": 0.05
 },
 "tournsize": {
    "description": "selection pressure (tournament size), n>=1 [default = 3]",
    "type": "int",
    "default": 3
 },
