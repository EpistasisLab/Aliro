{
 "primitives": { 
    "description": "GP primitives (operators/operands)",
    "type": "enum",
    "values": ["+","-","*","/","neg","ephemeral"],
    "default": ["+","-","*","/","neg","ephemeral"],
    "ui": {
        "style": "checkbox"
    }
 },
 "population-size": { 
    "description": "number of individuals",
    "type": "int",
    "default": 100,
    "ui": {
        "style": "radio",
        "choices": [10, 100, 1000]
    }
 },
 "generations": {
    "description": "number of generations, n>0 [default = 1000]",
    "type": "int",
    "default": 100,
    "ui": {
        "style": "radio",
        "choices": [10, 100, 1000]
    }
 },
 "crossover-probability": { 
    "description": "crossover probability, 0.0<=f<=1.0",
    "type": "float",
    "default": 0.9,
    "ui": {
        "style": "radio", 
        "choices": [0.1, 0.5, 0.9]
    }
 },
 "mutation-probability": { 
    "description": "0.0<=f<=1.0",
    "type": "float",
    "default": 0.1,
    "ui": {
        "style": "radio",
        "choices": [0.001, 0.01, 0.1]
    }
 },
 "seletion-pressure": { 
    "description": "selection pressure (tournament size), n>=1",
    "type": "int",
    "default": 3,
    "ui": {
        "style": "radio",
        "choices": [1, 2, 3]
    }
 },
 "elitism-size": { 
    "description": "elitism size, 0<=n<=1",
    "type": "float",
    "default": 1.0,
    "ui": {
        "style": "radio",
        "choices": [0.0, 0.5, 1.0]
    }
 },
 "maximum-size ": { 
    "description": "maximum program size",
    "type": "int",
    "default": 20,
    "ui": {
        "style": "radio",
        "choices": [5, 10, 20]
    }
 },
 "minimum-size": { 
    "description": "minimum program size",
    "type": "int",
    "default": 1,
    "ui": {
        "style": "radio",
        "choices": [1, 2, 3]
    }
 }
}
