        "/bin/bash",
        "/root/entrypoint.sh"
      ],
      "portMappings": [
        {
          "hostPort": 5081,
          "protocol": "tcp",
          "containerPort": 5081
        }
      ],
      "command": [],
      "mountPoints": [],
      "workingDirectory": "/opt",
      "memoryReservation": 1024,
      "volumesFrom": [],
      "image": "035856437156.dkr.ecr.us-east-2.amazonaws.com/machine:latest",
      "essential": true,
      "hostname": "machine",
      "name": "machine"
    }
  ],
  "family": "machine",
  "networkMode": "bridge"
}
