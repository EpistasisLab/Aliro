                                "Ref": "VPC"
                            },
                            "InternetGatewayId": {
                                "Ref": "InternetGateway"
                            }
                        }
                    },
                    "RouteTable": {
                        "Type": "AWS::EC2::RouteTable",
                        "Properties": {
                            "VpcId": {
                                "Ref": "VPC"
                            }
                        }
                    },

                    "Route": {
                        "Type": "AWS::EC2::Route",
                        "DependsOn": "AttachGateway",
                        "Properties": {
                            "RouteTableId": {
                                "Ref": "RouteTable"
