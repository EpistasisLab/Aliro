                            }
                        }
                    },

                    "FileSystem": {
                        "Type": "AWS::EFS::FileSystem",
                        "Properties": {}
                    },
                    "MountTarget": {
                        "Type": "AWS::EFS::MountTarget",
                        "Properties": {
                            "FileSystemId": {
                                "Ref": "FileSystem"
                            },
                            "SubnetId": {
                                "Ref": "Subnet"
                            },
                            "SecurityGroups": [{
                                "Ref": "SecurityGroup"
                            }]

                        }
                    },
                    "SecurityGroup": {
                        "Type": "AWS::EC2::SecurityGroup",
                        "Properties": {
                            "GroupDescription": "Security group allowing ingress from all IPs via TCP on port 22 only, and port 8888 for Jupyter notebook.",
                            "VpcId": {
                                "Ref": "VPC"
                            },
                            "SecurityGroupIngress": [{
                                "CidrIp": "0.0.0.0/0",
                                "FromPort": 22,
                                "IpProtocol": "tcp",
                                "ToPort": 22
                            }, {
                                "CidrIp": "0.0.0.0/0",
                                "FromPort": 443,
                                "IpProtocol": "tcp",
                                "ToPort": 443
