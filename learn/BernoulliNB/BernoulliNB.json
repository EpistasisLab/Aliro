		"default": 0.0,
		"ui": {
			"style": "radio",
			"choices": [0.0, 0.25, 0.5, 0.75, 1.0]
		}
	},
	"fit_prior": {
		"description": "whether to learn class prior probabilities or not",
		"type": "string",
		"default": true,
		"ui": {
			"style": "radio",
			"choices": [true, false]
		}
	}
