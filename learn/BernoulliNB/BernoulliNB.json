{
	"alpha": {
		"description": "additive (Laplace/Lidstone) smoothing parameter",
		"type": "float",
		"default": 1.0,
		"ui": {
			"style": "radio",
			"choices": [0.001, 0.01, 0.1, 1.0, 10.0, 100.0]
		}
	},
	"binarize": {
		"description": "threshold for binarizing (mapping to booleans) of sample features",
